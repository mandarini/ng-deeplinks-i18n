{
  "hosting": [
    {
      "target": "deeplinks",
      "source": ".",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "frameworksBackend": {
        "region": "europe-west1"
      },
      "i18n": {
        "root": "dist/deeplinks/browser"
      },
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
